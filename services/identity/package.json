    "migrate:down": "npx db-migrate down",
    "migrate:redo": "npx db-migrate redo",
    "migrate:create": "npx db-migrate create",
    "lint": "npx eslint ./",
    "lint:fix": "npx eslint --fix ./",
    "prettier": "npx prettier -w ./"
